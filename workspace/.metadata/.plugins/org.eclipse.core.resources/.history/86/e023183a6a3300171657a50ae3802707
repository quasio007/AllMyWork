package dem;

public class Try {

	public static void main(String[] args) throws Exception {
		// TODO Auto-generated method stub
Thread t1 =new Thread(new Runnable() {
	
	@Override
	public void run() {
		// TODO Auto-generated method stub
		Connection.getInstance().doConnect();
	}
});
Thread t2 =new Thread(new Runnable() {
	
	@Override
	public void run() {
		// TODO Auto-generated method stub
		Connection.getInstance().doConnect();
	}
});
t1.start();
t1.start();

t2.join();
t2.join();

	}

}
