package dead.lock;

public class Runner {

	public static void main(String[] args) throws Exception {
		DeadLockDemo deadLock =new DeadLockDemo();
		Thread t1 =new Thread(new Runnable() {
			
			@Override
			public void run() {
				// TODO Auto-generated method stub
				
			}
		});
		Thread t2 =new Thread(new Runnable() {
			
			@Override
			public void run() {
				// TODO Auto-generated method stub
				
			}
		});
		t1.start();
		t2.start();
		
		t1.join();
		t2.join();
		

	}

}
